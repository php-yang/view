{
    "name": "php-yang/view",
    "description": "Yang's view.",
    "keywords": [
        "php",
        "yang",
        "view",
        "template"
    ],
    "homepage": "http://github.com/php-yang/view",
    "require": {
        "php": ">=5.5"
    },
    "suggest": {
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "pengzhile",
            "email": "pengzhile@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Yang\\View\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yang\\View\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
